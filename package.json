{
  "name": "@edx/frontend-analytics",
  "version": "1.0.0-semantically-released",
  "description": "Frontend analytics client code for sending analytics events to Segment and (LMS) Tracking Logs.",
  "main": "dist/index.js",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "fedx-scripts babel src --out-dir dist --delete-dir-on-start --copy-files --source-maps",
    "lint": "fedx-scripts eslint --ext .js --ext .jsx .",
    "test": "fedx-scripts jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edx/frontend-analytics.git"
  },
  "author": "edX",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/edx/frontend-analytics/issues"
  },
  "homepage": "https://github.com/edx/frontend-analytics#readme",
  "devDependencies": {
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-angular": "^6.0.2",
    "@edx/frontend-build": "^2.0.0",
    "codecov": "^3.6.1",
    "husky": "^3.0.9"
  },
  "dependencies": {
    "form-urlencoded": "^3.0.0",
    "lodash.snakecase": "^4.1.1"
  }
}
